{
  "name": "argo-e2e",
  "version": "0.0.3",
  "scripts": {
    "interactive": "./scripts/interactive.js",
    "nightwatch": "nightwatch -c ./conf/dev.js",
    "nightwatch-chrome": "nightwatch -c ./conf/dev.js --env CHROME_LATEST",
    "nightwatch-ff": "nightwatch -c ./conf/dev.js --env FIREFOX_LATEST",
    "browserstack": "./scripts/local.runner.js -c ./conf/browserstack-default.js",
    "browserstack-all": "./scripts/local.runner.js -c ./conf/browserstack-chrome.js; ./scripts/local.runner.js -c ./conf/browserstack-firefox.js",
    "deploy": "git push && git push --tags",
    "clean": "rm -rf ./build",
    "type-check": "tsc -p ./tsconfig.json",
    "build": "npm run clean && npm run type-check",
    "postBuild": ""
  },
  "bin": {
    "geckodriver": "./node_modules/geckodriver/bin/geckodriver",
    "chromedriver": "./node_modules/chromedriver/bin/chromedriver"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-graphql-tag": "^2.1.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserstack-local": "^1.3.7",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
    "file-regex": "^3.0.1",
    "form-data": "^3.0.0",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.1",
    "inquirer": "^6.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.13",
    "nightwatch": "^1.2.4",
    "nodemon": "^1.19.0",
    "plop": "^2.3.0",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "recompose": "^0.30.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "shelljs": "^0.8.3",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "chromedriver": "^80.0.1",
    "geckodriver": "^1.19.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
